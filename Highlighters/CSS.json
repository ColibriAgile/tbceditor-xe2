               },
               {
                  "OpenToken":"/*",
                  "CloseToken":"*/",
                  "RegionType":"MultiLine"
               }
            ],
            "FoldRegion":[
               {
                  "OpenToken":"{",
                  "CloseToken":"}"
               }
            ]
         }
      ]
