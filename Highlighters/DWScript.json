							"old",
							"on",
							"operator",
							"or",
							"overload",
							"override",
							"pascal",
							"partial",
							"private",
							"program",
							"property",
							"protected",
							"public",
							"published",
							"raise",
							"record",
							"register",
							"reintroduce",
							"repeat",
							"require",
							"resourcestring",
							"sar",
							"sealed",
							"set",
							"shl",
							"shr",
							"static",
							"step",
