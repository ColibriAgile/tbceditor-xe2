{
    "Highlighter": {
        "Info": {
            "General": {
                "Version": "1.0",
                "Date": "05.06.2015",
                "Sample": [
                    "// Hello World in Pascal\r\n",
                    "\r\n",
                    "program HelloWorld;\r\n",
                    "begin\r\n",
                    "  writeln('Hello, world!');\r\n",
                    "end."
                ]
            },
            "Author": {
                "Name": "Eric Grange",
                "Mail": "eric@delphitools.info"
            }
        },
        "MainRules": {
			"Name": "DWScript",
            "Attributes": {
                "Element": "Editor"
            },
            "SubRules": {
                "Range": [
                    {
                        "Type": "Comment",
                        "Attributes": {
                            "Element": "Comment",
                            "ParentBackground": true
                        },
                        "TokenRange": {
                            "Open": "{",
                            "Close": "}"
                        }
                    },
                    {
                        "Type": "Comment",
