                        }
                    },
                    {
                        "Type": "Method",
                        "Attributes": {
                            "Element": "Method"
                        },
                        "TokenRange": {
                            "Open": "function"
                        },
                        "SubRules": {
                           "Range": [
                              {
                                "Type": "MethodName",
                                "Attributes": {
                                    "Element": "MethodName"
                                },
                                "Delimiters": ":(;",
                                "Properties": {
                                    "CloseParent": true,
                                    "CloseOnTerm": true,
                                    "SkipWhitespace": true
                                },
                                 "TokenRange": {
                                    "Open": " "
                                 }
                              }
                           ]
                        }
                    },
                    {
                        "Type": "Method",
                        "Attributes": {
                            "Element": "Method"
                        },
                        "TokenRange": {
                            "Open": "procedure"
                        },
                        "SubRules": {
                           "Range": [
                              {
                                "Type": "MethodName",
                                "Attributes": {
                                    "Element": "MethodName"
                                },
                                "Delimiters": "(;",
                                "Properties": {
                                    "CloseParent": true,
                                    "CloseOnTerm": true,
                                    "SkipWhitespace": true
                                },
                                 "TokenRange": {
                                    "Open": " "
                                 }
                              }
                           ]
                        }
                    },
                    {
                        "Type": "Method",
                        "Attributes": {
                            "Element": "Method"
                        },
                        "TokenRange": {
                            "Open": "method"
                        },
