							"try",
							"type",
							"unit",
							"until",
							"uses",
							"var",
							"virtual",
							"while",
							"with",
							"xor"
                        ],
                        "Attributes": {
                            "Element": "ReservedWord",
                            "ParentBackground": true
                        }
                    },
                    {
                        "Type": "Method",
                        "Words": ["function", "procedure"],
                        "Attributes": {
                            "Element": "Method"
                        }
                    },
                    {
                        "Type": "AssemblerReservedWord",
                        "Words": ["asm"],
                        "Attributes": {
                            "Element": "AssemblerReservedWord"
                        }
                    },
                    {
                        "Type": "Symbol",
                        "Words": [
                            "..",
                            ":",
                            ";",
                            "<",
                            ">",
                            "=",
                            "/",
                            "*",
                            ".",
                            ",",
                            "-",
                            "+",
                            "(",
                            ")",
                            "[",
                            "]"
