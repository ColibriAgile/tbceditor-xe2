                                "Attributes": {
                                    "Element": "MethodName"
                                },
                                "Delimiters": "(;",
                                "Properties": {
                                    "CloseParent": true,
                                    "CloseOnTerm": true,
                                    "SkipWhitespace": true
                                },
                                 "TokenRange": {
                                    "Open": " "
                                 }
                              }
                           ]
                        }
                    },
                    {
                        "Type": "ReservedWord",
                        "Attributes": {
                            "Element": "HighlightedBlock"
                        },
                        "TokenRange": {
                            "Open": "asm",
                            "Close": "end"
                        },
                        "SubRules": {
                            "Range": [
                                {
                                    "Type": "AssemblerComment",
                                    "Attributes": {
                                        "Element": "Comment",
                                        "ParentBackground": true
                                    },
                                    "TokenRange": {
                                        "Open": "{",
                                        "Close": "}"
                                    }
                                },
                                {
                                    "Type": "AssemblerComment",
                                    "Attributes": {
                                        "Element": "Comment",
                                        "ParentBackground": true
                                    },
                                    "TokenRange": {
                                        "Open": "(*",
                                        "Close": "*)"
                                    }
                                },
                                {
                                    "Type": "AssemblerComment",
                                    "Attributes": {
                                        "Element": "Comment",
                                        "ParentBackground": true
                                    },
                                    "Properties": {
                                        "CloseOnEol": true
                                    },
                                    "TokenRange": {
                                        "Open": "//"
                                    }
                                },
                                {
                                    "Type": "Directive",
                                    "Attributes": {
                                        "Element": "Directive",
                                        "ParentBackground": true
                                    },
                                    "TokenRange": {
                                        "Open": "{$",
                                        "Close": "}"
                                    }
                                }
                            ],
                            "KeyList": [
                                {
                                    "Type": "AssemblerReservedWord",
                                    "Words": ["end"],
                                    "Attributes": {
                                        "Element": "ReservedWord",
                                        "ParentBackground": true
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "Type": "HexNumber",
                        "Attributes": {
                            "Element": "HexNumber",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnTerm": true,
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "$",
                            "Close": " "
                        }
                    },
                    {
                        "Type": "Character",
                        "Attributes": {
                            "Element": "Number",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnTerm": true,
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "#",
                            "Close": " "
                        }
                    },
                    {
                        "Type": "Address",
